{
  "name": "focus-english",
  "version": "1.0.0",
  "description": "Focus English - Cursos de Inglés Especializados para Trabajo, Viajes y Exámenes",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "cf:dev": "wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "start": "node server.js",
    "start:next": "next start -p ${PORT:-3000}",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "verify-openai": "ts-node scripts/verify-openai-setup.ts",
    "verify-supabase": "ts-node scripts/verify-supabase-setup.ts",
    "generate-audio": "ts-node --esm scripts/generate-course-audio.ts",
    "generate-audio:test": "ts-node --esm scripts/generate-course-audio.ts test",
    "generate-audio:all": "ts-node --esm scripts/generate-course-audio.ts all"
  },
  "dependencies": {
    "@auth/supabase-adapter": "^1.11.1",
    "@google-cloud/text-to-speech": "^6.4.0",
    "@stripe/stripe-js": "^8.6.1",
    "@supabase/supabase-js": "^2.90.1",
    "bcryptjs": "^3.0.3",
    "elevenlabs-node": "^2.0.3",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.562.0",
    "next": "^15.1.3",
    "next-auth": "^4.24.13",
    "openai": "^6.16.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "resend": "^6.7.0",
    "stripe": "^20.1.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vercel": "^47.0.4",
    "wrangler": "^4.59.2"
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "english",
    "courses",
    "learning",
    "education",
    "language",
    "professional-english",
    "travel-english",
    "exam-preparation"
  ],
  "author": "Focus English",
  "license": "UNLICENSED"
}
