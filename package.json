{
  "name": "focus-english",
  "version": "1.0.0",
  "description": "Focus English - Cursos de Inglés Especializados para Trabajo, Viajes y Exámenes",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001 -H 127.0.0.1",
    "build": "next build",
    "start": "next start -p ${PORT:-3000}",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test && npm run test:e2e",
    "verify-openai": "ts-node scripts/verify-openai-setup.ts",
    "generate-audio": "ts-node --esm scripts/generate-course-audio.ts",
    "generate-audio:test": "ts-node --esm scripts/generate-course-audio.ts test",
    "generate-audio:all": "ts-node --esm scripts/generate-course-audio.ts all",
    "generate-audio:a1-expansion": "ts-node --esm scripts/generate-a1-expansion-audio.ts",
    "validate-exercises": "node scripts/validate-exercises.mjs",
    "validate-courses": "npx tsx scripts/validate-courses.mjs",
    "validate-exercise-types": "node scripts/validate-exercise-types.mjs",
    "validate-weeks": "node scripts/validate-week.mjs src/content/weeks/*.json",
    "audit-seo": "node scripts/audit-blog-seo.mjs"
  },
  "dependencies": {
    "@google-cloud/text-to-speech": "^6.4.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-tabs": "^1.1.13",
    "@stripe/stripe-js": "^8.6.1",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.90.1",
    "@tailwindcss/typography": "^0.5.19",
    "@types/canvas-confetti": "^1.9.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "canvas-confetti": "^1.9.4",
    "clsx": "^2.1.1",
    "elevenlabs-node": "^2.0.3",
    "framer-motion": "^12.29.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.562.0",
    "next": "15.5.11",
    "next-themes": "^0.4.6",
    "openai": "^6.16.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "resend": "^6.7.0",
    "stripe": "^20.1.2",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.5.11",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "playwright": "^1.57.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "english",
    "courses",
    "learning",
    "education",
    "language",
    "professional-english",
    "travel-english",
    "exam-preparation"
  ],
  "author": "Focus English",
  "license": "UNLICENSED"
}
