{
  "build": {
    "command": "npm install && npm run build",
    "cwd": "/",
    "output_directory": ".next"
  },
  "env": {
    "NODE_VERSION": "20",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "preview": {
    "alias": ["preview.focusenglish.pages.dev"]
  },
  "production": {
    "branch": "main"
  }
}
